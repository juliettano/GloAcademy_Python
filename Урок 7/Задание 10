counter = 0
n = int(input())
for i in range(1, n + 1):
    number = int(input())
    if number % 2 != 0:
        counter = counter + 1

if counter == n:
    print('YES')
else:
    print('NO')

2-й рабочий вариант

flag = 0
n = int(input())
for i in range(n):
    number = int(input())
    if number % 2 == 0:
        flag = 1

if flag != 0:
    print('NO')
else:
    print('YES')
